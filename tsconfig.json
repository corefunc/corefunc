{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": false,
    "alwaysStrict": true,
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "ES2018",
      "ES2018.AsyncGenerator",
      "ES2018.AsyncIterable",
      "ES2018.Intl",
      "ES2018.Promise",
      "ES2018.Regexp",
      "ES2019",
      "ES2019.Array",
      "ES2019.Object",
      "ES2019.String",
      "ES2019.Symbol",
      "ES2020",
      "ES2020.BigInt",
      "ES2020.Promise",
      "ES2020.String",
      "ES2020.Symbol.WellKnown",
      "dom"
    ],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "newLine": "lf",
    "sourceMap": false,
    "target": "ES2020",
    "typeRoots": ["node_modules/@types"],
    "types": ["node"]
  },
  "exclude": ["node_modules", "tests"],
  "include": [
    "array",
    "cast",
    "check",
    "cli",
    "collection",
    "convert",
    "crypt",
    "date",
    "dom",
    "fp",
    "func",
    "generate",
    "hash",
    "high-order",
    "is",
    "json",
    "lang",
    "number",
    "object",
    "promise",
    "prototype",
    "regex",
    "string",
    "text",
    "time",
    "v8",
    "valid",
    "var"
  ]
}
