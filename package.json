  },
  "scripts": {
    "doc": "typedoc --options typedoc.json",
    "lint": "eslint ./**/*.{mj,t}s",
    "lint:dir": "eslint ./**/*.{cj,mj,t}s",
    "lint:md": "remark .",
    "package:audit": "npm audit --json | npx npm-audit-html",
    "package:check": "npx depcheck",
    "package:upgrade": "npx npm-check --update",
    "test": "jest --passWithNoTests",
    "test:cjs": "node ./tests/index.cjs",
    "test:coverage": "jest --collect-coverage",
    "test:mjs": "node --experimental-modules ./tests/index.mjs"
